{
  "name": "neosolva/php-api-client",
  "type": "library",
  "homepage": "https://github.com/Neosolva/php-api-client",
  "description": "Neosolva API client",
  "license": "proprietary",
  "authors": [
    {
      "name": "Joanis ROUANET",
      "email": "jrouanet@agrege.com"
    }
  ],
  "autoload": {
    "psr-4": {"Neosolva\\Component\\Api\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "Neosolva\\Component\\Api\\Tests\\": "tests/" }
  },
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "symfony/http-client": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/cache": "^6.0",
    "symfony/property-access": "^6.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1"
  },
  "config": {
    "sort-packages": true
  }
}
